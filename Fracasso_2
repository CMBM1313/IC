 = nova_matriz\n","      X=X+M\n","      Humilhacao(phi, phiprime, target, T, M, X)\n","    elif p >= 500:\n","      break\n","\n","\n","Cronos = []\n","M = 250\n","T = 250\n","V = 1.0\n","target = 1e-5\n","X = 0\n","\n","\n","phi = np.zeros([M, M, M], float)\n","\n","phi[0, :, 0] = V\n","phi[M - 1, :,0] = V\n","phi[:, 0, 0] = V\n","phi[:, M - 1, 0] = V\n","phi[(M//4):(((3*M)//4)), (M//4):(((3*M)//4)), 0] = -V\n","\n","\n","phiprime = np.empty([M, M, M], float)\n","n = 0\n","delta = 1.0\n","\n","phiprime[0, :, 0] = phi[0, :, 0]\n","phiprime[M - 1, :, 0] = phi[M - 1, :, 0]\n","phiprime[:, 0,0] = phi[:, 0, 0]\n","phiprime[:, M - 1, 0] = phi[:, M - 1, 0]\n","phiprime[(M//4):((3*M)//4), (M//4):((3*M)//4), 0] = phi[(M//4):((3*M)//4), (M//4):((3*M)//4), 0]\n","\n","Humilhacao(phi, phiprime, target, T, M, X)\n","\n","plt.imshow(phi[:,:,0])\n","plt.colorbar()\n","plt.show()\n","\n","plt.imshow(phi[:,:,1])\n","plt.colorbar()\n","plt.show()\n","\n","plt.imshow(phi[:,:,(T-1)//5])\n","plt.colorbar()\n","plt.show()\n","\n","plt.imshow(phi[:,:,(T-1)])\n","plt.colorbar()\n","plt.show()\n","\n","print(T)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":391},"id":"HKPcPTh3W2jm","executionInfo":{"status":"error","timestamp":1734237438721,"user_tz":180,"elapsed":293235,"user":{"displayName":"Charles Magnus Brito Maya","userId":"15827407630013413864"}},"outputId":"df12ecbc-2514-4c18-b223-ea2e22f18f43","collapsed":true},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["passei aqui\n","passei aqui\n"]},{"output_type":"error","ename":"IndexError","evalue":"index 750 is out of bounds for axis 2 with size 750","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mIndexError\u001b[0m                                Traceback (most recent call last)","\u001b[0;32m<ipython-input-6-97929f4917d0>\u001b[0m in \u001b[0;36m<cell line: 72>\u001b[0;34m()\u001b[0m\n\u001b[1;32m   